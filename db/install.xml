<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/notices/db" VERSION="20250102" COMMENT="XMLDB file for Moodle blocks/notices"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_notices" COMMENT="Contains notices for this block instance">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="instanceid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Block instance Id"/>
        <FIELD NAME="visible" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="hidden=0, visible=1"/>
        <FIELD NAME="title" TYPE="char" LENGTH="64" NOTNULL="true" SEQUENCE="false" COMMENT="Title of the notice"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
